{
    "name": "AutoJoomer",
    "version": "2.0.3",
    "manifest_version": 2,
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "jquery.js",
            "script.js"
        ],
        "persistent": true
    },
    "content_scripts": [
        {
            "matches": [
                "https://lms.iiitkottayam.ac.in/",
                "https://lms.iiitkottayam.ac.in/login/index.php",
                "https://lms.iiitkottayam.ac.in/login/index.php/#",
                "https://lms.iiitkottayam.ac.in/mod/bigbluebuttonbn/*"
            ],
            "js": [
                "jquery.js",
                "logger.js"
            ]
        }
    ]
}
