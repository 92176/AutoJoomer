{
    "name": "AutoJoomer",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "jquery.js",
            "script.js"
        ],
        "persistent": true
    }
}